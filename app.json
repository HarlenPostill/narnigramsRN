{
  "expo": {
    "name": "Narnigrams",
    "slug": "narnigrams",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/narnigrams-logo.png",
    "scheme": "narnigrams",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "icon": {
        "light": "./assets/images/narnigrams-icon-light.png",
        "dark": "./assets/images/narnigrams-icon-dark.png"
      },
      "bundleIdentifier": "com.hrln-interaction.narnigrams"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/narnigrams-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0062FF",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
