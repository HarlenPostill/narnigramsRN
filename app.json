{
  "expo": {
    "name": "Narnigrams",
    "slug": "narnigrams",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/narnigrams-logo.png",
    "scheme": "narnigrams",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "icon": {
        "light": "./assets/images/narnigrams-icon-light.png",
        "dark": "./assets/images/narnigrams-icon-dark.png"
      },
      "bundleIdentifier": "com.hrln-interaction.narnigrams"
    },
    "android": {
      "icon": "./assets/images/narnigrams-icon-light.png",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hrlninteraction.narnigrams"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/narnigrams-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0062FF",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-asset",
        {
          "assets": ["./assets/words.txt"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
